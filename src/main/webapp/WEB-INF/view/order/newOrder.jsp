<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %>
<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<!-- Navigation -->
<jsp:include page="../module/navi.jsp" />


<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->

    <section class="content-header">
        <h1>
            Dodaj powiadomienie o nowych ofertach
            <small>...</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li><a href="#">Tables</a></li>
            <li class="active">Powiadomienia</li>
        </ol>
    </section>

    <section class="content">
        <div class="raw">
            <div class="col-md-6">
                <!-- general form elements -->
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">Nowe powiadomienie</h3>
                    </div>
                    <!-- /.box-header -->
                    <!-- form start -->
                    <form:form action="#" commandName="newOrder" modelAttribute="newOrder" role="form" lpformnum="1" _lpchecked="1" method="post">
                        <div class="box-body">

                            <div style="display: none;">
                                <form:input path="idorder" type="text" class="form-control" id="exampleInputEmail1" placeholder="Id" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;"/>

                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail1">Email address</label>
                                <form:input path="email" type="text" class="form-control" id="exampleInputEmail1" placeholder="Email" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;"/>
                            </div>

                            <div class="form-group">
                                <label for="exampleInputEmail1">Liczba pokoi</label>
                                <form:input path="numrooms" type="text" class="form-control" id="exampleInputEmail1" placeholder="Liczba pokoi" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;"/>
                            </div>




                            <div class="form-group">
                                <label for="exampleInputEmail1">Miasto</label>
                                <form:select path="location" cssClass="form-control">
                                    <c:if test="${newOrder.location!=null}">
                                        <form:option value="0" label="${newOrder.location}" />
                                    </c:if>
                                    <c:if test="${newOrder.location==null}">
                                        <form:option value="0" label="Wybierz miasto" />
                                    </c:if>
                                    <form:options items="${cits_lhm}" />
                                </form:select>
                                </td>
                            </div>

                            <div class="form-group">
                                <label for="exampleInputEmail1">Dzielnica</label>
                                <form:input path="district" type="text" class="form-control" id="exampleInputEmail1" placeholder="Dzielnica" autocomplete="off" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;"/>
                            </div>

                        </div>
                        <div class="box box-success">
                            <div class="box-header with-border">
                                <h3 class="box-title">Cena od ... - do ...</h3>
                            </div>
                            <div class="box-body">
                                <div class="row">

                                    <div class="col-xs-4">
                                        <form:input path="price_from" type="text" class="form-control" placeholder="od"/>
                                    </div>
                                    <div class="col-xs-4">
                                        <form:input path="price_to" type="text" class="form-control" placeholder="do"/>
                                    </div>
                                </div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box-body -->

                        <div class="box-footer">
                            <button type="submit" class="btn btn-primary" value = "Submit">Submit</button>
                        </div>
                    </form:form>
                </div>
                <!-- /.box -->
            </div>
        </div>
    </section>

</div>
<!-- /.content-wrapper -->


<!-- Navigation -->
